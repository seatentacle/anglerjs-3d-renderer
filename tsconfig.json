{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "lib": [
      "ES6",
      "DOM"
    ],
    "outDir": "dist/",
    "baseUrl": "./",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "paths": {
      "components/*": ["src/components/*"],
      "constants/*": ["src/constants/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
